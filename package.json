{
  "name": "strider-gitlab",
  "version": "1.2.6",
  "description": "A gitlab provider for strider",
  "scripts": {
    "test": "npm run lint && npm run tests",
    "lint": "jshint --verbose lib/",
    "tests": "mocha -R spec test/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-gitlab.git"
  },
  "keywords": [
    "git",
    "gitlab",
    "strider"
  ],
  "author": {
    "name": "Martin Ericson <martin@devbox.com>"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "strider": {
    "id": "gitlab",
    "title": "GitLab",
    "type": "provider",
    "hosted": true,
    "config": {
      "controller": "GitLabCtrl"
    },
    "accountConfig": {
      "setupLink": "/account/#provider-gitlab",
      "template": "config/accountConfig.html"
    },
    "webapp": "lib/webapp.js",
    "worker": "lib/worker.js",
    "inline_icon": "git-square"
  },
  "devDependencies": {
    "eslint": "^3.1.1",
    "expect.js": "~0.3.1",
    "jshint": "~2.9.2",
    "mocha": "~3.0.0",
    "nock": "^8.0.0"
  },
  "dependencies": {
    "async": "~2.0.0-rc.3",
    "debug": "~2.2.0",
    "gravatar": "^1.0.6",
    "lodash": "~4.16.4",
    "parse-link-header": "^0.4.1",
    "proxyquire": "^1.7.1",
    "step": "0.0.6",
    "strider-git": "^0.2.4",
    "superagent": "~2.3.0"
  },
  "bugs": {
    "url": "https://github.com/Strider-CD/strider-gitlab/issues"
  },
  "homepage": "https://github.com/Strider-CD/strider-gitlab"
}
